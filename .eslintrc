{
  "extends": ["plugin:prettier/recommended"],

  "plugins": ["prettier"],

  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },

  "parserOptions": {
    "sourceType": "module"
  },

  "env": {
    "es2020": true,
    "node": true,
    "jest": true
  },

  "parser": "react-scripts/node_modules/babel-eslint"
}
